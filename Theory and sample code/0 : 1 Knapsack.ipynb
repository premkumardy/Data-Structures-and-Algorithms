    "        return 0\n",
    "    \n",
    "    dfs(i+1,currProfit+profit[i],capacity-weight[i])\n",
    "\n",
    "\n",
    "    dfs(i+1,currProfit,capacity)\n",
    "\n",
    "dfs(0,0,capacity)"
